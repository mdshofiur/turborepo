{
  "extends": "tsconfig/nextjs.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
  ],

  "compilerOptions": {
    "paths": {
      "@/*": ["./components/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
  },
  "plugins":[{"name":"next"}], 
  "exclude": [
    "node_modules"
  ]
}
